From f9b2e63cffcfc0100ce7bf817417e84a5f61bbf4 Mon Sep 17 00:00:00 2001
From: Frank Wang <frank.wang@rock-chips.com>
Date: Thu, 21 Mar 2019 09:42:36 +0800
Subject: [PATCH] ARM: dts: amend rk322x-usb2phy to rk3228-usb2phy

Change-Id: If47746bf2bc1736c5c9e891829c1013c2449e1f4
Signed-off-by: Frank Wang <frank.wang@rock-chips.com>
---
 arch/arm/boot/dts/rk3128x.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/rk3128x.dtsi b/arch/arm/boot/dts/rk3128x.dtsi
index 6472edea7a61..7733726f327e 100644
--- a/arch/arm/boot/dts/rk3128x.dtsi
+++ b/arch/arm/boot/dts/rk3128x.dtsi
@@ -355,7 +355,7 @@
 		};
 
 		u2phy0: usb2-phy@760 {
-			compatible = "rockchip,rk322x-usb2phy";
+			compatible = "rockchip,rk3228-usb2phy";
 			reg = <0x0760 0x0c>;
 			clocks = <&cru SCLK_OTGPHY0>;
 			clock-names = "phyclk";
@@ -382,7 +382,7 @@
 		};
 
 		u2phy1: usb2-phy@800 {
-			compatible = "rockchip,rk322x-usb2phy";
+			compatible = "rockchip,rk3228-usb2phy";
 			reg = <0x0800 0x0c>;
 			clocks = <&cru SCLK_OTGPHY1>;
 			clock-names = "phyclk";
-- 
