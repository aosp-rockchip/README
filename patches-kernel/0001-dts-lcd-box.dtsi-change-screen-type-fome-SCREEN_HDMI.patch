From baf374531b7f18256e2961f86cf260ce278b963e Mon Sep 17 00:00:00 2001
From: huang zhibao <hzb@rock-chips.com>
Date: Sun, 29 Mar 2015 17:02:09 +0800
Subject: [PATCH] dts: lcd-box.dtsi change screen-type fome SCREEN_HDMI to
 SCREEN_RGB

---
 arch/arm/boot/dts/lcd-box.dtsi | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/boot/dts/lcd-box.dtsi b/arch/arm/boot/dts/lcd-box.dtsi
index 4907735e621d..e6fca36081c9 100755
--- a/arch/arm/boot/dts/lcd-box.dtsi
+++ b/arch/arm/boot/dts/lcd-box.dtsi
@@ -34,7 +34,7 @@
 		disp_timings: display-timings {
 			native-mode = <&timing0>;
 			timing0: timing0 {
-				screen-type = <SCREEN_HDMI>;
+				screen-type = <SCREEN_RGB>;
 				out-face    = <OUT_P888>;
 				color-mode = <COLOR_YCBCR>;
 				clock-frequency = <74250000>;
@@ -55,7 +55,7 @@
 				swap-gb = <0>;
                      	};
                      	timing1: timing1 {
-				screen-type = <SCREEN_HDMI>;
+				screen-type = <SCREEN_RGB>;
 				out-face    = <OUT_P888>;
 				color-mode = <COLOR_YCBCR>;
 				clock-frequency = <148500000>;
@@ -76,7 +76,7 @@
 				swap-gb = <0>;
                      	};
                      	timing2: timing2 {
-				screen-type = <SCREEN_HDMI>;
+				screen-type = <SCREEN_RGB>;
 				out-face    = <OUT_P888>;
 				color-mode = <COLOR_YCBCR>;
 				clock-frequency = <297000000>;
-- 
2.35.3

