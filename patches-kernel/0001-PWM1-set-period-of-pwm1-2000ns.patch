Subject: [PATCH] PWM1: set period of pwm1(2000ns)

---
 arch/arm/boot/dts/rk3288-tb.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/rk3288-tb.dts b/arch/arm/boot/dts/rk3288-tb.dts
index bc471d56a024..7e0a743f3aa3 100755
--- a/arch/arm/boot/dts/rk3288-tb.dts
+++ b/arch/arm/boot/dts/rk3288-tb.dts
@@ -59,7 +59,7 @@
 
 	pwm_regulator {
 		compatible = "rockchip_pwm_regulator";
-		pwms = <&pwm1 0 25000>;
+		pwms = <&pwm1 0 2000>;
 		rockchip,pwm_id= <1>;
 		rockchip,pwm_voltage_map= <925000 950000 975000 1000000 1025000 1050000 1075000 1100000 1125000 1150000 1175000 1200000 1225000 1250000 1275000 1300000 1325000 1350000 1375000 1400000>;
 		rockchip,pwm_voltage= <1000000>;
