From 9c62f69bf0cb376b67bd2c61c8f188cac4534681 Mon Sep 17 00:00:00 2001
From: xbw <xbw@rock-chips.com>
Date: Tue, 18 Mar 2014 13:43:42 +0800
Subject: [PATCH] SDMMC: add the SDIO_IRQ for sdio

---
 arch/arm/boot/dts/rk3188-tb.dts | 2 +-
 arch/arm/boot/dts/rk3288-tb.dts | 1 +
 2 files changed, 2 insertions(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/rk3188-tb.dts b/arch/arm/boot/dts/rk3188-tb.dts
index fa6f6bf590e4..f7acc25455dc 100755
--- a/arch/arm/boot/dts/rk3188-tb.dts
+++ b/arch/arm/boot/dts/rk3188-tb.dts
@@ -182,7 +182,7 @@
 		
   	supports-highspeed;		
 	supports-sdio;
-
+	cap-sdio-irq;
 	status = "okay";
 };
 
diff --git a/arch/arm/boot/dts/rk3288-tb.dts b/arch/arm/boot/dts/rk3288-tb.dts
index 81c20da96f05..fcd87254a14f 100755
--- a/arch/arm/boot/dts/rk3288-tb.dts
+++ b/arch/arm/boot/dts/rk3288-tb.dts
@@ -80,6 +80,7 @@
 		
   	supports-highspeed;		
 	supports-sdio;
+	cap-sdio-irq;
 
 	status = "diabled";
 };
-- 
2.35.3

