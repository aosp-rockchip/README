-u4Byte 
-ConvertTo_dB(
-	u4Byte 	Value)
-{
-	u1Byte i;
-	u1Byte j;
-	u4Byte dB;
-
-	Value = Value & 0xFFFF;
-	
-	for (i=0;i<8;i++)
-	{
-		if (Value <= dB_Invert_Table[i][11])
-		{
-			break;
-		}
-	}
-
-	if (i >= 8)
-	{
-		return (96);	// maximum 96 dB
-	}
-
-	for (j=0;j<12;j++)
-	{
-		if (Value <= dB_Invert_Table[i][j])
-		{
-			break;
-		}
-	}
-
-	dB = i*12 + j + 1;
-
-	return (dB);
-}
