From 399b794dd7b6950198bd564b2017c46bc6d3c1c7 Mon Sep 17 00:00:00 2001
From: "yangjie@rock-chips.com" <yangjie@rock-chips.com>
Date: Fri, 21 Mar 2014 15:18:33 +0800
Subject: [PATCH] dts: rk3288-p977 && rk3288-tb

---
 arch/arm/boot/dts/rk3288-p977.dts | 14 ++++++++++++++
 arch/arm/boot/dts/rk3288-tb.dts   | 14 ++++++++++++++
 2 files changed, 28 insertions(+)

diff --git a/arch/arm/boot/dts/rk3288-p977.dts b/arch/arm/boot/dts/rk3288-p977.dts
index 4de2dd3de752..48e1b47787f9 100755
--- a/arch/arm/boot/dts/rk3288-p977.dts
+++ b/arch/arm/boot/dts/rk3288-p977.dts
@@ -192,6 +192,20 @@
 	/*   gpios = <&gpio0 GPIO_A7 GPIO_ACTIVE_HIGH>; */
 		status = "disable";
 	};
+
+	CW2015@62 {
+		compatible = "cw201x";
+		reg = <0x62>;
+		dc_det_gpio = <&gpio0 GPIO_B0 GPIO_ACTIVE_LOW>;
+		bat_low_gpio = <&gpio0 GPIO_A7 GPIO_ACTIVE_LOW>;
+		chg_ok_gpio = <&gpio0 GPIO_B1 GPIO_ACTIVE_HIGH>;
+		bat_config_info = <0x15 0x42 0x60 0x59 0x52 0x58 0x4D 0x48 0x48 0x44 0x44 0x46 0x49 0x48 0x32
+			0x24 0x20 0x17 0x13 0x0F 0x19 0x3E 0x51 0x45 0x08 0x76 0x0B 0x85 0x0E 0x1C 0x2E 0x3E 0x4D 0x52 0x52
+			0x57 0x3D 0x1B 0x6A 0x2D 0x25 0x43 0x52 0x87 0x8F 0x91 0x94 0x52 0x82 0x8C 0x92 0x96 0xFF 0x7B 0xBB
+			0xCB 0x2F 0x7D 0x72 0xA5 0xB5 0xC1 0x46 0xAE>;
+		is_dc_charge = <1>;
+		is_usb_charge = <0>;
+	};
 };
 
 &i2c1 {
diff --git a/arch/arm/boot/dts/rk3288-tb.dts b/arch/arm/boot/dts/rk3288-tb.dts
index da3817afa128..6cde07b834b5 100755
--- a/arch/arm/boot/dts/rk3288-tb.dts
+++ b/arch/arm/boot/dts/rk3288-tb.dts
@@ -209,6 +209,20 @@
 	/*   gpios = <&gpio0 GPIO_A7 GPIO_ACTIVE_HIGH>; */
 		status = "okay";
 	};
+
+	CW2015@62 {
+		compatible = "cw201x";
+		reg = <0x62>;
+		dc_det_gpio = <&gpio0 GPIO_B0 GPIO_ACTIVE_LOW>;
+		bat_low_gpio = <&gpio0 GPIO_A7 GPIO_ACTIVE_LOW>;
+		chg_ok_gpio = <&gpio0 GPIO_B1 GPIO_ACTIVE_HIGH>;
+		bat_config_info = <0x15 0x42 0x60 0x59 0x52 0x58 0x4D 0x48 0x48 0x44 0x44 0x46 0x49 0x48 0x32
+			0x24 0x20 0x17 0x13 0x0F 0x19 0x3E 0x51 0x45 0x08 0x76 0x0B 0x85 0x0E 0x1C 0x2E 0x3E 0x4D 0x52 0x52
+			0x57 0x3D 0x1B 0x6A 0x2D 0x25 0x43 0x52 0x87 0x8F 0x91 0x94 0x52 0x82 0x8C 0x92 0x96 0xFF 0x7B 0xBB
+			0xCB 0x2F 0x7D 0x72 0xA5 0xB5 0xC1 0x46 0xAE>;
+		is_dc_charge = <1>;
+		is_usb_charge = <0>;
+	};
 };
 
 &i2c1 {
