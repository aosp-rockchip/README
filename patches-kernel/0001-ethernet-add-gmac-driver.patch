+		} else if (!strncmp(opt, "dma_rxsize:", 11)) {
+			if (kstrtoint(opt + 11, 0, &dma_rxsize))
+				goto err;
+		} else if (!strncmp(opt, "buf_sz:", 7)) {
+			if (kstrtoint(opt + 7, 0, &buf_sz))
