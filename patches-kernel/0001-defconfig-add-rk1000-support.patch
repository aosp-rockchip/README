Subject: [PATCH] defconfig: add rk1000 support

---
 arch/arm64/configs/rockchip_defconfig | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/arch/arm64/configs/rockchip_defconfig b/arch/arm64/configs/rockchip_defconfig
index 1650eeaf8a4d..169fd528c3b5 100644
--- a/arch/arm64/configs/rockchip_defconfig
+++ b/arch/arm64/configs/rockchip_defconfig
@@ -402,6 +402,8 @@ CONFIG_MIPI_DSI=y
 CONFIG_RK32_MIPI_DSI=y
 CONFIG_RK_HDMI=y
 CONFIG_RK_TVENCODER=y
+CONFIG_RK1000_TVOUT=y
+CONFIG_RK1000_TVOUT_CVBS=y
 CONFIG_ROCKCHIP_RGA=y
 CONFIG_ROCKCHIP_RGA2=y
 CONFIG_LOGO=y
@@ -421,6 +423,7 @@ CONFIG_SND_RK_SOC_HDMI_I2S=y
 CONFIG_SND_RK_SOC_SPDIF_CARD=y
 CONFIG_SND_RK_SOC_RT5631=y
 CONFIG_SND_RK_SOC_RT3224=y
+CONFIG_SND_RK_SOC_RK1000=y
 CONFIG_SND_RK_SOC_RK312X=y
 CONFIG_HIDRAW=y
 CONFIG_UHID=y
