From 29ebc4486f5709dda959e7fdcc01c2e3792ef393 Mon Sep 17 00:00:00 2001
From: Zheng Yang <zhengyang@rock-chips.com>
Date: Thu, 16 Apr 2015 09:35:50 +0800
Subject: [PATCH] rk3368-box.dts: modify fb display policy to
 DISPLAY_POLICY_BOX_TEMP.

Signed-off-by: Zheng Yang <zhengyang@rock-chips.com>
---
 arch/arm64/boot/dts/rk3368-box.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rk3368-box.dts b/arch/arm64/boot/dts/rk3368-box.dts
index d09b0b0ae583..94fd7ba8777e 100755
--- a/arch/arm64/boot/dts/rk3368-box.dts
+++ b/arch/arm64/boot/dts/rk3368-box.dts
@@ -594,7 +594,7 @@
 &fb {
 	rockchip,disp-mode = <NO_DUAL>;
 	rockchip,uboot-logo-on = <1>;
-	rockchip,disp-policy = <DISPLAY_POLICY_BOX>;
+	rockchip,disp-policy = <DISPLAY_POLICY_BOX_TEMP>;
 };
 
 &disp_timings {
-- 
2.35.3

