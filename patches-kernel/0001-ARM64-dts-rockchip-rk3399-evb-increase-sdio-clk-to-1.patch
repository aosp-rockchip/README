From 668cbe2f661d8756e91ae81039caa1b0283e2767 Mon Sep 17 00:00:00 2001
From: Weilong Gao <gwl@rock-chips.com>
Date: Tue, 6 Sep 2016 10:40:11 +0800
Subject: [PATCH] ARM64: dts: rockchip: rk3399-evb: increase sdio clk to 150M

Change-Id: Id35a5ce58517fb48d2a3f348eb7c4571b83d3f5e
Signed-off-by: Weilong Gao <gwl@rock-chips.com>
---
 arch/arm64/boot/dts/rockchip/rk3399-evb.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3399-evb.dtsi b/arch/arm64/boot/dts/rockchip/rk3399-evb.dtsi
index dc9d88f8f77e..23afa5dfb412 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399-evb.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3399-evb.dtsi
@@ -285,8 +285,8 @@
 };
 
 &sdio0 {
-	clock-frequency = <50000000>;
-	clock-freq-min-max = <200000 50000000>;
+	clock-frequency = <150000000>;
+	clock-freq-min-max = <200000 150000000>;
 	supports-sdio;
 	bus-width = <4>;
 	disable-wp;
-- 
2.35.3

