From 96aa175dae8a4861d56728122fd213a90db46fb6 Mon Sep 17 00:00:00 2001
From: "buluess.li" <buluess.li@rock-chips.com>
Date: Tue, 27 Jun 2017 16:44:17 +0800
Subject: [PATCH] arm: dts: rk3288-evb: set i2c4 clock frequency to 400khz

Change-Id: I849a99d147e9336ba81cd25c92594ffb1289cb53
Signed-off-by: buluess.li <buluess.li@rock-chips.com>
---
 arch/arm/boot/dts/rk3288-evb.dtsi | 1 +
 1 file changed, 1 insertion(+)

diff --git a/arch/arm/boot/dts/rk3288-evb.dtsi b/arch/arm/boot/dts/rk3288-evb.dtsi
index d8a22d699dbc..3e1f38168292 100644
--- a/arch/arm/boot/dts/rk3288-evb.dtsi
+++ b/arch/arm/boot/dts/rk3288-evb.dtsi
@@ -307,6 +307,7 @@
 
 &i2c4 {
 	status = "okay";
+	clock-frequency = <400000>;
 
 	gsl3673@40 {
 		compatible = "GSL,GSL3673";
-- 
2.35.3

