From 73dd2b8bf57f4cd15806d00a819c2b3d6aa2bf9a Mon Sep 17 00:00:00 2001
From: xbw <xbw@rock-chips.com>
Date: Mon, 17 Mar 2014 17:55:48 +0800
Subject: [PATCH] SDMMC: delete the unuse aliases

---
 arch/arm/boot/dts/rk3188.dtsi | 3 ---
 arch/arm/boot/dts/rk3288.dtsi | 4 ----
 2 files changed, 7 deletions(-)

diff --git a/arch/arm/boot/dts/rk3188.dtsi b/arch/arm/boot/dts/rk3188.dtsi
index d6421bdf9cd3..5b6f9f560da4 100755
--- a/arch/arm/boot/dts/rk3188.dtsi
+++ b/arch/arm/boot/dts/rk3188.dtsi
@@ -14,9 +14,6 @@
 	rockchip,sram = <&sram>;
 
 	aliases {
-		emmc = &emmc;
-		sdmmc = &sdmmc;
-		sdio = &sdio;
 		serial0 = &uart0;
 		serial1 = &uart1;
 		serial2 = &uart2;
diff --git a/arch/arm/boot/dts/rk3288.dtsi b/arch/arm/boot/dts/rk3288.dtsi
index 7a1c4ecc5659..382d3420e9eb 100755
--- a/arch/arm/boot/dts/rk3288.dtsi
+++ b/arch/arm/boot/dts/rk3288.dtsi
@@ -13,10 +13,6 @@
 	interrupt-parent = <&gic>;
 
 	aliases {
-		emmc = &emmc;
-		sdmmc= &sdmmc;
-		sdio = &sdio;
-		sdio1= &sdio1
 		serial2 = &uart_dbg;
 		i2c0 = &i2c0;
 		i2c1 = &i2c1;
