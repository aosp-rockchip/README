@@ -31,28 +31,28 @@ static struct rk29_keys_button key_button[] = {
 	{
 		.desc	= "menu",
 		.code	= EV_MENU,
-		.adc_value	= 155,
+		.adc_value	= 133,
 		.gpio = INVALID_GPIO,
 		.active_low = PRESS_LEV_LOW,
