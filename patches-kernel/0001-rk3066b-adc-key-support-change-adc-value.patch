 		.desc	= "esc",
 		.code	= KEY_BACK,
-		.adc_value	= 386,
+		.adc_value	= 333,
 		.gpio = INVALID_GPIO,
 		.active_low = PRESS_LEV_LOW,
 	},
 	{
 		.desc	= "camera",
 		.code	= KEY_CAMERA,
-		.adc_value	= 827,
+		.adc_value	= 742,
