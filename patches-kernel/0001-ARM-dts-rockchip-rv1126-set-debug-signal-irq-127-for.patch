Date: Fri, 9 Oct 2020 11:51:56 +0800
Subject: [PATCH] ARM: dts: rockchip: rv1126: set debug-signal irq 127 for fiq
 debugger

Change-Id: I40a5846a197bf8b90cb9141dae0e982e68a5e2b1
Signed-off-by: Huibin Hong <huibin.hong@rock-chips.com>
---
 arch/arm/boot/dts/rv1126.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/rv1126.dtsi b/arch/arm/boot/dts/rv1126.dtsi
index f8ba601c7f6f..3499ebcb53dd 100644
--- a/arch/arm/boot/dts/rv1126.dtsi
+++ b/arch/arm/boot/dts/rv1126.dtsi
@@ -283,7 +283,7 @@
 		rockchip,wake-irq = <0>;
 		rockchip,irq-mode-enable = <0>;
 		rockchip,baudrate = <1500000>;  /* Only 115200 and 1500000 */
-		interrupts = <GIC_SPI 93 IRQ_TYPE_LEVEL_HIGH>;
+		interrupts = <GIC_SPI 127 IRQ_TYPE_LEVEL_HIGH>;
 		status = "disabled";
 	};
 
