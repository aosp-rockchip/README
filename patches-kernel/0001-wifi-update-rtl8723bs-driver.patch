+/*
+	ask FW to Reset sync register at Beacon early interrupt
+*/
+u8 rtl8723b_reset_tsf(_adapter *padapter, u8 reset_port )
+{
+	u8	buf[2];
+	u8	res=_SUCCESS;
+
+_func_enter_;
+	if (IFACE_PORT0==reset_port) {
+		buf[0] = 0x1; buf[1] = 0;
+
+	} else{
+		buf[0] = 0x0; buf[1] = 0x1;
+	}
+	FillH2CCmd8723B(padapter, H2C_8723B_RESET_TSF, 2, buf);
+_func_exit_;
+
+	return res;
+}
