+			} 
+	#endif
+#endif
+			else 
+			{
+//				if (txop == 0) {
+//#if defined(CONFIG_USB_HCI) || defined(CONFIG_SDIO_HCI)
+//					disable_cfe = 1;
+//#endif
+//#ifdef CONFIG_PCI_HCI
