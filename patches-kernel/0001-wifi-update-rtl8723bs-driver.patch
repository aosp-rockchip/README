+
+		bCommon = TRUE;
+	}
+	else if(bWifiConnected && 
+			(BT_8821A_CSR_2ANT_BT_STATUS_IDLE == pCoexDm->btStatus) )
+	{		
+		bLowPwrDisable = FALSE;
