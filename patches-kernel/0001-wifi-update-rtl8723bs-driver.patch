+	
+	//3(9) KEK, KCK
+	RsvdPageLoc.LocGTKInfo = TotalPageNum;
+	_rtw_memcpy(ReservedPagePacket+BufIndex-TxDescLen, kck, RTW_KCK_LEN);
+	_rtw_memcpy(ReservedPagePacket+BufIndex-TxDescLen+RTW_KCK_LEN, kek, RTW_KEK_LEN);
