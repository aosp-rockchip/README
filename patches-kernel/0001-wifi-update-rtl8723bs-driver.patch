+	u8		tmp = 0x3e;
+	u32 	res,i=0;
+	u4Byte	   ArrayLen    = sizeof(Array_kfreemap)/sizeof(u32);
+	pu4Byte    Array	   = Array_kfreemap;
+	u4Byte v1=0,v2=0,target=0; 
+	//DBG_871X("+%s value: 0x%02x+\n", __func__, value);
+#if defined(CONFIG_RTL8723A)
+	if (value & BIT0) {
