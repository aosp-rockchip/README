+			ODM_Write4Byte(pDM_Odm, ODM_EDCA_BE_PARAM,  (BE_TXOP*2 << 16) | (cw_max << 12) | (4 << 8) | (sifs_time + 3 * slot_time));
+		#endif
+		#endif
+/*		
+		if (priv->pshare->txop_enlarge == 0xe) {
+#if defined(CONFIG_USB_HCI) || defined(CONFIG_SDIO_HCI)
