+				}
+				else
+				#if(DM_ODM_SUPPORT_TYPE==ODM_AP)&&(defined LOW_TP_TXOP)
+				{
+#if defined(CONFIG_USB_HCI) || defined(CONFIG_SDIO_HCI)
