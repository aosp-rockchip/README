From 909dbed165ee73527eab589a64144d9b89ca014f Mon Sep 17 00:00:00 2001
From: kfx <kfx@rock-chips.com>
Date: Sat, 5 Jan 2013 10:40:58 +0800
Subject: [PATCH] iomux: update iomux name

---
 arch/arm/mach-rk2928/include/mach/iomux-rk2928.h | 2 +-
 arch/arm/mach-rk30/include/mach/iomux-rk30.h     | 4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/mach-rk2928/include/mach/iomux-rk2928.h b/arch/arm/mach-rk2928/include/mach/iomux-rk2928.h
index b64b0feba2b9..8cfdf9de240c 100644
--- a/arch/arm/mach-rk2928/include/mach/iomux-rk2928.h
+++ b/arch/arm/mach-rk2928/include/mach/iomux-rk2928.h
@@ -119,7 +119,7 @@ enum{
 	/* GPIO3_A */
 	/* GPIO3_B */
 	/* GPIO3_C */
-	GPIO3_C1 = 0x3c10, OTG_DRVVBUS, 
+	GPIO3_C1 = 0x3c10, OTG_DRV_VBUS, 
 
 	/* GPIO3_D */
 	GPIO3_D7 = 0x3d70, TEST_CLK_OUT, 
diff --git a/arch/arm/mach-rk30/include/mach/iomux-rk30.h b/arch/arm/mach-rk30/include/mach/iomux-rk30.h
index 595fd50a0257..d82a3258b7c2 100644
--- a/arch/arm/mach-rk30/include/mach/iomux-rk30.h
+++ b/arch/arm/mach-rk30/include/mach/iomux-rk30.h
@@ -105,7 +105,7 @@ enum{
 	GPIO2_D4 = 0x2d40, SMC_BLSN0, 
 	GPIO2_D5 = 0x2d50, SMC_BLSN1, 
 	GPIO2_D6 = 0x2d60, SMC_CS1, 
-	GPIO2_D7 = 0x2d70, TEST_CLOCK_OUT, 
+	GPIO2_D7 = 0x2d70, TEST_CLK_OUT, 
 
 	/* GPIO3_A */
 	GPIO3_A0 = 0x3a00, MMC0_RSTNOUT, 
@@ -353,7 +353,7 @@ enum{
 
 	/* GPIO6_A */
 	/* GPIO6_B */
-	GPIO6_B7 = 0x6b70, TEST_CLOCK_OUT, 
+	GPIO6_B7 = 0x6b70, TEST_CLK_OUT, 
 
 	/* GPIO6_C */
 	/* GPIO6_D */
