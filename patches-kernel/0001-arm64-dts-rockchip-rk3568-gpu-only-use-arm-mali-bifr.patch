From a400cac16883c42e15b17e1308cabe8859e10133 Mon Sep 17 00:00:00 2001
From: Zhen Chen <chenzhen@rock-chips.com>
Date: Mon, 1 Feb 2021 14:12:26 +0800
Subject: [PATCH] arm64: dts: rockchip: rk3568: gpu: only use
 "arm,mali-bifrost" in 'compatible'

Signed-off-by: Zhen Chen <chenzhen@rock-chips.com>
Change-Id: Idd3e21ecc3d53d97200bd46b849a22e4f7777333
---
 arch/arm64/boot/dts/rockchip/rk3568.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3568.dtsi b/arch/arm64/boot/dts/rockchip/rk3568.dtsi
index fec422d67365..be87f241bb64 100644
--- a/arch/arm64/boot/dts/rockchip/rk3568.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3568.dtsi
@@ -1055,7 +1055,7 @@
 	};
 
 	gpu: gpu@fde60000 {
-		compatible = "arm,malit602", "arm,malit60x", "arm,malit6xx", "arm,mali-midgard";
+		compatible = "arm,mali-bifrost";
 		reg = <0x0 0xfde60000 0x0 0x4000>;
 
 		interrupts = <GIC_SPI 39 IRQ_TYPE_LEVEL_HIGH>,
-- 
2.35.3

