From f0f7db17ce342273bdf5c3c48f25a30b9e43c9cb Mon Sep 17 00:00:00 2001
From: wdc <wdc@rock-chips.com>
Date: Wed, 19 Mar 2014 11:13:22 +0800
Subject: [PATCH] rk3288: change i2c aliases

---
 arch/arm/boot/dts/rk3288.dtsi | 12 ++++++------
 1 file changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/arm/boot/dts/rk3288.dtsi b/arch/arm/boot/dts/rk3288.dtsi
index e0a602244b75..bae204eea367 100755
--- a/arch/arm/boot/dts/rk3288.dtsi
+++ b/arch/arm/boot/dts/rk3288.dtsi
@@ -18,12 +18,12 @@
 		serial2 = &uart_dbg;
 		serial3 = &uart_gps;
 		serial4 = &uart_exp;
-		i2c_pmu = &i2c0;
-		i2c_sensor = &i2c1;
-		i2c_audio = &i2c2;
-		i2c_cam = &i2c3;
-		i2c_tp = &i2c4;
-		i2c5_hdmi = &i2c5;
+		i2c0 = &i2c0;
+		i2c1 = &i2c1;
+		i2c2 = &i2c2;
+		i2c3 = &i2c3;
+		i2c4 = &i2c4;
+		i2c5 = &i2c5;
 		lcdc0 = &lcdc0;
 		lcdc1 = &lcdc1;
 	};
