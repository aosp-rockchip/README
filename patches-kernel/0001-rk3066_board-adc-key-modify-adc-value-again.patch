 	{
 		.desc	= "esc",
 		.code	= KEY_BACK,
-		.adc_value	= 402,
+		.adc_value	= 334,
 		.gpio = INVALID_GPIO,
 		.active_low = PRESS_LEV_LOW,
 	},
 	{
 		.desc	= "camera",
 		.code	= KEY_CAMERA,
-		.adc_value	= 854,
+		.adc_value	= 743,
