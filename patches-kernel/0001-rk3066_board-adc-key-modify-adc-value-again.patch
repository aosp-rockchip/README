@@ -82,28 +82,28 @@ static struct rk29_keys_button key_button[] = {
 	{
 		.desc	= "menu",
 		.code	= EV_MENU,
-		.adc_value	= 163,
+		.adc_value	= 135,
 		.gpio = INVALID_GPIO,
 		.active_low = PRESS_LEV_LOW,
 	},
 	{
 		.desc	= "home",
 		.code	= KEY_HOME,
-		.adc_value	= 652,
+		.adc_value	= 550,
 		.gpio = INVALID_GPIO,
 		.active_low = PRESS_LEV_LOW,
