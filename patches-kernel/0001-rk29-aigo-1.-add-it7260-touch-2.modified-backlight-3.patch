+
+/* set the it7260 registe,used i2c bus*/
+static int it7260_set_regs(struct i2c_client *client, u8 reg, u8 const buf[], unsigned short len)
+{
+	int ret; 
+	ret = i2c_master_reg8_send(client, reg, buf, (int)len, IT7260_IIC_SPEED);
+	return ret;
+}
