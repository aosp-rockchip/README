+static int it7260_read_regs(struct i2c_client *client, u8 reg, u8 buf[], unsigned len)
+{
+	int ret; 
+	ret = i2c_master_reg8_recv(client, reg, buf, len, IT7260_IIC_SPEED);
+	return ret; 
+}
