+
+    gpio_direction_output(TOUCH_RESET_PIN, 0);
+    mdelay(100);
+    gpio_set_value(TOUCH_RESET_PIN,GPIO_LOW);
+    mdelay(100);
+    gpio_set_value(TOUCH_RESET_PIN,GPIO_HIGH);
+	
+    gpio_direction_output(TOUCH_INT_PIN, 1);    
