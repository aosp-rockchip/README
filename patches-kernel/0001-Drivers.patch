+    {
+        GTP_INFO("Create proc entry success!");
+        //goodix_proc_entry->write_proc = goodix_tool_write;
+        //goodix_proc_entry->read_proc = goodix_tool_read;
+    }
+
+    return SUCCESS;
+}
