From cfc8fde1bb9174bfd521f88cbfd4add62f5bbd27 Mon Sep 17 00:00:00 2001
From: Cody Xie <xkd@rock-chips.com>
Date: Fri, 7 Jun 2013 10:44:55 +0800
Subject: [PATCH] rk3188 ds1006h : defconfig remove CONFIG_CLK_SWITCH_TO_32K

---
 arch/arm/configs/rk3188_ds1006h_defconfig      | 2 +-
 arch/arm/configs/rk3188_ds1006h_v1_1_defconfig | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/configs/rk3188_ds1006h_defconfig b/arch/arm/configs/rk3188_ds1006h_defconfig
index 5397bb94328d..305f6b151d2f 100644
--- a/arch/arm/configs/rk3188_ds1006h_defconfig
+++ b/arch/arm/configs/rk3188_ds1006h_defconfig
@@ -23,7 +23,6 @@ CONFIG_MODULE_FORCE_UNLOAD=y
 CONFIG_ARCH_RK3188=y
 # CONFIG_DDR_TEST is not set
 CONFIG_RK_CLOCK_PROC=y
-CONFIG_CLK_SWITCH_TO_32K=y
 CONFIG_RK_USB_UART=y
 CONFIG_MACH_RK3188_DS1006H=y
 CONFIG_FIQ_DEBUGGER=y
@@ -323,6 +322,7 @@ CONFIG_SND=y
 # CONFIG_SND_ARM is not set
 CONFIG_SND_SOC=y
 CONFIG_SND_RK29_SOC=y
+CONFIG_SND_I2S_USE_33V=y
 CONFIG_SND_I2S_DMA_EVENT_STATIC=y
 CONFIG_SND_RK29_SOC_RT5631=y
 CONFIG_SND_RK29_CODEC_SOC_SLAVE=y
diff --git a/arch/arm/configs/rk3188_ds1006h_v1_1_defconfig b/arch/arm/configs/rk3188_ds1006h_v1_1_defconfig
index 8d55f5d53993..e130753ca03c 100644
--- a/arch/arm/configs/rk3188_ds1006h_v1_1_defconfig
+++ b/arch/arm/configs/rk3188_ds1006h_v1_1_defconfig
@@ -23,7 +23,6 @@ CONFIG_MODULE_FORCE_UNLOAD=y
 CONFIG_ARCH_RK3188=y
 # CONFIG_DDR_TEST is not set
 CONFIG_RK_CLOCK_PROC=y
-CONFIG_CLK_SWITCH_TO_32K=y
 CONFIG_RK_USB_UART=y
 CONFIG_MACH_RK3188_DS1006H=y
 CONFIG_FIQ_DEBUGGER=y
@@ -322,6 +321,7 @@ CONFIG_SND=y
 # CONFIG_SND_ARM is not set
 CONFIG_SND_SOC=y
 CONFIG_SND_RK29_SOC=y
+CONFIG_SND_I2S_USE_33V=y
 CONFIG_SND_I2S_DMA_EVENT_STATIC=y
 CONFIG_SND_RK29_SOC_RK610=y
 CONFIG_SND_RK29_CODEC_SOC_SLAVE=y
