From 238b00ba9068171a0d9993cc7b24596acfa9f2d6 Mon Sep 17 00:00:00 2001
From: David Wu <david.wu@rock-chips.com>
Date: Fri, 3 Apr 2020 15:57:31 +0800
Subject: [PATCH] ARM: dts: rv1108: Remove "rockchip,grf" property except i2c2

Signed-off-by: David Wu <david.wu@rock-chips.com>
Change-Id: I0ca4699e2a031dbe53e77cbceba61a5a47257048
---
 arch/arm/boot/dts/rv1108.dtsi | 3 ---
 1 file changed, 3 deletions(-)

diff --git a/arch/arm/boot/dts/rv1108.dtsi b/arch/arm/boot/dts/rv1108.dtsi
index 56caf87ca9c4..291dfa07e6c0 100644
--- a/arch/arm/boot/dts/rv1108.dtsi
+++ b/arch/arm/boot/dts/rv1108.dtsi
@@ -160,7 +160,6 @@
 		clock-names = "i2c", "pclk";
 		pinctrl-names = "default";
 		pinctrl-0 = <&i2c1_xfer>;
-		rockchip,grf = <&grf>;
 		status = "disabled";
 	};
 
@@ -188,7 +187,6 @@
 		clock-names = "i2c", "pclk";
 		pinctrl-names = "default";
 		pinctrl-0 = <&i2c3_xfer>;
-		rockchip,grf = <&grf>;
 		status = "disabled";
 	};
 
@@ -369,7 +367,6 @@
 		clock-names = "i2c", "pclk";
 		pinctrl-names = "default";
 		pinctrl-0 = <&i2c0_xfer>;
-		rockchip,grf = <&grf>;
 		status = "disabled";
 	};
 
