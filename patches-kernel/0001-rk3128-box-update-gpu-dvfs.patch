Subject: [PATCH] rk3128 box: update gpu dvfs

---
 arch/arm/boot/dts/rk3128-box-rk88.dts | 6 +++---
 arch/arm/boot/dts/rk3128-box.dts      | 6 +++---
 2 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/arm/boot/dts/rk3128-box-rk88.dts b/arch/arm/boot/dts/rk3128-box-rk88.dts
index 12bbe5fa7169..46b6ccde424a 100755
--- a/arch/arm/boot/dts/rk3128-box-rk88.dts
+++ b/arch/arm/boot/dts/rk3128-box-rk88.dts
@@ -89,9 +89,9 @@
 	&clk_gpu_dvfs_table {
 		operating-points = <
 			/* KHz    uV */
-			200000 1100000
-			300000 1100000
-			400000 1150000
+			200000 950000
+			300000 975000
+			400000 1075000
 			>;
 		status="okay";
 	};
