From 5e4bf4ff0c20610f329d5d31bf95ac5883718410 Mon Sep 17 00:00:00 2001
From: Finley Xiao <finley.xiao@rock-chips.com>
Date: Wed, 7 Mar 2018 17:52:25 +0800
Subject: [PATCH] arm64: dts: rockchip: px30: Enable pvtm

Change-Id: Ib2a0fe5bc2a9e80ea48d35fe526a9efe5df586e7
Signed-off-by: Finley Xiao <finley.xiao@rock-chips.com>
---
 arch/arm64/boot/dts/rockchip/px30.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/px30.dtsi b/arch/arm64/boot/dts/rockchip/px30.dtsi
index 35c9de4730ba..74ef00065fcc 100644
--- a/arch/arm64/boot/dts/rockchip/px30.dtsi
+++ b/arch/arm64/boot/dts/rockchip/px30.dtsi
@@ -299,7 +299,7 @@
 			compatible = "rockchip,px30-pmu-pvtm";
 			clocks = <&pmucru SCLK_PVTM_PMU>;
 			clock-names = "pmu";
-			status = "disabled";
+			status = "okay";
 		};
 	};
 
@@ -426,7 +426,7 @@
 			compatible = "rockchip,px30-pvtm";
 			clocks = <&cru SCLK_PVTM>;
 			clock-names = "core";
-			status = "disabled";
+			status = "okay";
 		};
 	};
 
-- 
