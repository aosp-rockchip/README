From 5b3f63d63774a6b812e1444c2587a90b41df485f Mon Sep 17 00:00:00 2001
From: chenzhen <chenzhen@rock-chips.com>
Date: Tue, 9 May 2017 11:42:36 +0800
Subject: [PATCH] ARM: rockchip_defconfig: enable driver for Mali400 GPU

Change-Id: Idcccf39fc0a5d2d340325e1b3445c8c0d3dcbcc3
Signed-off-by: chenzhen <chenzhen@rock-chips.com>
---
 arch/arm/configs/rockchip_defconfig | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/arch/arm/configs/rockchip_defconfig b/arch/arm/configs/rockchip_defconfig
index fe7e547e3bec..04fdf7170665 100644
--- a/arch/arm/configs/rockchip_defconfig
+++ b/arch/arm/configs/rockchip_defconfig
@@ -397,6 +397,10 @@ CONFIG_ROCKCHIP_INNO_HDMI=y
 CONFIG_ROCKCHIP_LVDS=y
 CONFIG_DRM_PANEL_SIMPLE=y
 CONFIG_DRM_DW_HDMI_I2S_AUDIO=y
+CONFIG_MALI400=y
+# CONFIG_MALI400_PROFILING is not set
+CONFIG_MALI_SHARED_INTERRUPTS=y
+CONFIG_MALI_DT=y
 CONFIG_MALI_DEVFREQ=y
 CONFIG_MALI_MIDGARD=y
 CONFIG_MALI_EXPERT=y
