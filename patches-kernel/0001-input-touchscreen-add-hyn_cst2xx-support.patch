+	}
+
+	printk("cst2xx_proc_write_foobar*********cmd:%d*****%d******len:%d .\r\n", cmd[0], cmd[1], len);
+
+	if (cmd[0] == 0)
+	{
+		 pdata = kzalloc(sizeof(char)*length, GFP_KERNEL);
+		if(pdata == NULL)
+		{
+			printk("zalloc GFP_KERNEL memory fail.\n");
