+static void hard_reset_chip(struct hyn_ts *ts, u16 ms)
+{
+	int ret=0;
+	int retry=0;
+	unsigned char buf[4];
+
+	buf[0] = 0xD1;
+	buf[1] = 0x0e;
+	while(retry++ < 3) {
+		ret = cst2xx_i2c_write(ts->client, buf, 2);
+		if (ret > 0) break;
+		msleep(2);
+	}
+
+	msleep(ms);
+}
