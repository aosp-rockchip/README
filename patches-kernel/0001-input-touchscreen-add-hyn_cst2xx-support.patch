+
+	}
+	else if (cmd[0] == 2)
+	{
+		//cst2xx_touch_release();
+		CMDIndex = cmd[1];
+	}
+	else if (cmd[0] == 3)
+	{
+		CMDIndex = 0;
+	}
+
+	return count;
