+{
+	struct hyn_ts *ts = container_of(tp_d, struct hyn_ts, tp);
+
+#ifdef HYN_GESTURE
+	gsl_lcd_flag = 1;
+#endif
+
+	printk("[CST2XX] Enter %s\n", __func__);
+	hyn_ts_suspend(&ts->client->dev);
+	return 0;
+}
