+static int hyn_ts_late_resume(struct tp_device *tp_d)
+{
+	struct hyn_ts *ts = container_of(tp_d, struct hyn_ts, tp);
+
+#ifdef HYN_GESTURE
+	gsl_lcd_flag = 0;
+	gsl_gesture_flag = 0;
+#endif
+
+	printk("[CST2XX] Enter %s\n", __func__);
+	hyn_ts_resume(&ts->client->dev);
+	return 0;
+}
