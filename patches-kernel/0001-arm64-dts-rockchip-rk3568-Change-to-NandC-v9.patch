From 39f742d4f97a6a01d34c2ff886e522288fccbc98 Mon Sep 17 00:00:00 2001
From: Jon Lin <jon.lin@rock-chips.com>
Date: Tue, 1 Dec 2020 16:11:03 +0800
Subject: [PATCH] arm64: dts: rockchip: rk3568: Change to NandC v9

Change-Id: Iacfdb94df576eac58a57507c6def62faae2adcc1
Signed-off-by: Jon Lin <jon.lin@rock-chips.com>
---
 arch/arm64/boot/dts/rockchip/rk3568.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3568.dtsi b/arch/arm64/boot/dts/rockchip/rk3568.dtsi
index b38938a49364..6e20d5b9ffbf 100644
--- a/arch/arm64/boot/dts/rockchip/rk3568.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3568.dtsi
@@ -1890,7 +1890,7 @@
 	};
 
 	nandc0: nandc@fe330000 {
-		compatible = "rockchip,rk-nandc";
+		compatible = "rockchip,rk-nandc-v9";
 		reg = <0x0 0xfe330000 0x0 0x4000>;
 		interrupts = <GIC_SPI 70 IRQ_TYPE_LEVEL_HIGH>;
 		nandc_id = <0>;
-- 
