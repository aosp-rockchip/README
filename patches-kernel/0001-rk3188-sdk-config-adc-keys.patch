From ea8be30a04b0ae273c0c19ecbab14e5aba10290e Mon Sep 17 00:00:00 2001
From: wuhao <wuhao@wuhao@rock-chips.com>
Date: Thu, 28 Mar 2013 15:46:40 +0800
Subject: [PATCH] rk3188 sdk:config adc keys

---
 arch/arm/mach-rk3188/board-rk3188-sdk.c | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/mach-rk3188/board-rk3188-sdk.c b/arch/arm/mach-rk3188/board-rk3188-sdk.c
index 068b78a9e57b..2d8004d5dede 100644
--- a/arch/arm/mach-rk3188/board-rk3188-sdk.c
+++ b/arch/arm/mach-rk3188/board-rk3188-sdk.c
@@ -2109,7 +2109,7 @@ static struct rk29_keys_button key_button[] = {
         {
                 .desc   = "vol-",
                 .code   = KEY_VOLUMEDOWN,
-		.adc_value      = 900,
+		.adc_value      = 174,
                 .gpio   = INVALID_GPIO,
                 .active_low = PRESS_LEV_LOW,
         },
@@ -2130,21 +2130,21 @@ static struct rk29_keys_button key_button[] = {
 	{
                 .desc   = "menu",
                 .code   = EV_MENU,
-                .adc_value      = 133,
+                .adc_value      = 360,
                 .gpio = INVALID_GPIO,
                 .active_low = PRESS_LEV_LOW,
         },
         {
                 .desc   = "home",
                 .code   = KEY_HOME,
-                .adc_value      = 550,
+                .adc_value      = 750,
                 .gpio = INVALID_GPIO,
                 .active_low = PRESS_LEV_LOW,
         },
         {
                 .desc   = "esc",
                 .code   = KEY_BACK,
-                .adc_value      = 333,
+                .adc_value      = 564,
 		.gpio = INVALID_GPIO,
 		.active_low = PRESS_LEV_LOW,
 	},
