Subject: [PATCH] arm64: dts: rockchip: enable cdn dp machine driver for
 rk3399-evb-cros

Change-Id: I97bebe09b83d5a3d9a964cf7dbaf63dbb11daffb
Signed-off-by: Chris Zhong <zyw@rock-chips.com>
---
 arch/arm64/boot/dts/rockchip/rk3399-evb-cros.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3399-evb-cros.dtsi b/arch/arm64/boot/dts/rockchip/rk3399-evb-cros.dtsi
index c450f38f63b7..bb5d901dadf4 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399-evb-cros.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3399-evb-cros.dtsi
@@ -73,7 +73,7 @@
 	};
 
 	sound {
-		compatible = "rockchip,rk3399-gru-sound";
+		compatible = "rockchip,cdndp-sound";
 		rockchip,cpu = <&i2s2>;
 		rockchip,codec = <&cdn_dp>;
 	};
