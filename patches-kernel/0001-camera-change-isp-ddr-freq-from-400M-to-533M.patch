From a9f4a9f43961d144b31532ce379cd5fa09a01acf Mon Sep 17 00:00:00 2001
From: zyc <zyc@rock-chips.com>
Date: Wed, 25 Jun 2014 08:44:42 +0800
Subject: [PATCH] camera: change isp ddr freq from 400M to 533M

---
 arch/arm/boot/dts/rk3288-p977.dts      | 2 +-
 arch/arm/boot/dts/rk3288-p977_8846.dts | 2 +-
 arch/arm/boot/dts/rk3288-tb.dts        | 2 +-
 arch/arm/boot/dts/rk3288-tb1.dts       | 2 +-
 4 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/rk3288-p977.dts b/arch/arm/boot/dts/rk3288-p977.dts
index aea86f790bb3..1f9d623214db 100755
--- a/arch/arm/boot/dts/rk3288-p977.dts
+++ b/arch/arm/boot/dts/rk3288-p977.dts
@@ -598,7 +598,7 @@
 		SYS_STATUS_PERFORMANCE  528000
 		SYS_STATUS_DUALVIEW	400000
 		SYS_STATUS_BOOST	324000
-		SYS_STATUS_ISP		400000
+		SYS_STATUS_ISP		533000
 		>;
 	auto-freq-table = <
 		240000
diff --git a/arch/arm/boot/dts/rk3288-p977_8846.dts b/arch/arm/boot/dts/rk3288-p977_8846.dts
index 7a7c6b1678ff..f7f60dc276f8 100755
--- a/arch/arm/boot/dts/rk3288-p977_8846.dts
+++ b/arch/arm/boot/dts/rk3288-p977_8846.dts
@@ -657,7 +657,7 @@
 		SYS_STATUS_PERFORMANCE  528000
 		SYS_STATUS_DUALVIEW	400000
 		SYS_STATUS_BOOST	324000
-		SYS_STATUS_ISP		400000
+		SYS_STATUS_ISP		533000
 		>;
 	auto-freq-table = <
 		240000
diff --git a/arch/arm/boot/dts/rk3288-tb.dts b/arch/arm/boot/dts/rk3288-tb.dts
index 1ff6ae730612..cb94cba70120 100755
--- a/arch/arm/boot/dts/rk3288-tb.dts
+++ b/arch/arm/boot/dts/rk3288-tb.dts
@@ -745,7 +745,7 @@
 		SYS_STATUS_PERFORMANCE  528000
 		SYS_STATUS_DUALVIEW	400000
 		SYS_STATUS_BOOST	324000
-		SYS_STATUS_ISP		400000
+		SYS_STATUS_ISP		533000
 		>;
 	auto-freq-table = <
 		240000
diff --git a/arch/arm/boot/dts/rk3288-tb1.dts b/arch/arm/boot/dts/rk3288-tb1.dts
index 9d9e2a2c0b39..435ee259335a 100755
--- a/arch/arm/boot/dts/rk3288-tb1.dts
+++ b/arch/arm/boot/dts/rk3288-tb1.dts
@@ -701,7 +701,7 @@ rockchip,power_type = <GPIO>;
 		SYS_STATUS_PERFORMANCE  528000
 		SYS_STATUS_DUALVIEW	400000
 		SYS_STATUS_BOOST	324000
-		SYS_STATUS_ISP		400000
+		SYS_STATUS_ISP		533000
 		>;
 	auto-freq-table = <
 		240000
