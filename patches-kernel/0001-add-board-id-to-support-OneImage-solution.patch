+		memset(buf, 0, MAX_BUF_LEN);
+		for(i=AREA_ID_NULL; i<AREA_ID_NUMS+1; i++)
+		{			
+			if(area_id_name_start->type && (strlen(area_id_name_start->locale_language) > 0))
