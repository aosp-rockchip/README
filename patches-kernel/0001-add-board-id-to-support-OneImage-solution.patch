+static int board_id_vendor_data_check(char *pbuf)
+{
+	int sum = 0;
+	int i = 0;
+	int temp = 0;
+
+	//for(i=DEVICE_TYPE_NULL; i<DEVICE_NUM_TYPES; i++)
+	//printk("buf[%d]=0x%x\n",i, pbuf[i]);
+
+	temp = ((int)(pbuf[DEVICE_TYPE_SUM] << 24) | ((int)(pbuf[DEVICE_TYPE_SUM+1] << 16)) | ((int)(pbuf[DEVICE_TYPE_SUM+2] << 8)) | (int)(pbuf[DEVICE_TYPE_SUM+3]));
+	
+	for(i=DEVICE_TYPE_AREA; i<DEVICE_NUM_TYPES; i++)
+	sum += pbuf[i];
+
+	if((temp != sum) || (temp == 0) || (sum == 0))
+	{
+		printk("%s:sum=0x%x, temp=0x%x\n", __func__, sum, temp);
+		return -1;
+	}
+
+	return 0;
+}
