+	int j = 0, reg_num;
+	u8 *ptemp, *phead;
+	int reg_length;
+#endif
+
+	cnt = 0;
+	if (sensor_task_lock(client, 1) < 0)
+		goto sensor_write_array_end;
+    while (regarray[i].reg != SEQUENCE_END) {
