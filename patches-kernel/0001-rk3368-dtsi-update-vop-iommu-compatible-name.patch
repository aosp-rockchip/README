From 68c0a19fed6f168e415136f9fd0ad4cb725eb7e4 Mon Sep 17 00:00:00 2001
From: hjc <hjc@rock-chips.com>
Date: Tue, 10 Mar 2015 15:05:27 +0800
Subject: [PATCH] rk3368 dtsi: update vop iommu compatible name

Signed-off-by: hjc <hjc@rock-chips.com>
---
 arch/arm64/boot/dts/rk3368.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rk3368.dtsi b/arch/arm64/boot/dts/rk3368.dtsi
index adfdb51b94ce..08e2b22cbe83 100755
--- a/arch/arm64/boot/dts/rk3368.dtsi
+++ b/arch/arm64/boot/dts/rk3368.dtsi
@@ -1091,7 +1091,7 @@
 
 	vop_mmu {
 		dbgname = "vop";
-		compatible = "rockchip,vop_mmu";
+		compatible = "rockchip,vopb_mmu";
 		reg = <0x0 0xff930300 0x0 0x100>;
 		interrupts = <GIC_SPI 15 IRQ_TYPE_LEVEL_HIGH>;
 		interrupt-names = "vop_mmu";
-- 
2.35.3

