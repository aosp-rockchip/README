+		flash0-supply = <&dcdc2_reg>;  /*FLASH0_VDD*/
+		wifi-supply = <&ldo7_reg>;     /*APIO2_VDD*/
+		audio-supply = <&dcdc2_reg>;   /*APIO3_VDD*/
+		sdcard-supply = <&ldo1_reg>;   /*SDMMC0_VDD*/
