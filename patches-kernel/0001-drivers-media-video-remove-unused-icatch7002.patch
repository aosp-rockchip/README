-	char *ptr = NULL;
-	if (count >= sizeof(g_procbuff)) {
-		DEBUG_TRACE("%s no space\n", __FUNCTION__);
-		return -ENOSPC;
-	}
-
-	if (copy_from_user(g_procbuff, buffer, count)) {
-		DEBUG_TRACE("%s copy from user fail %d\n", __FUNCTION__, count);
-		return -EFAULT;
