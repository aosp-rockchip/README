@@ -41,7 +41,6 @@ CONFIG_HMP_FAST_CPU_MASK="4-7"
 CONFIG_HMP_SLOW_CPU_MASK="0-3"
 CONFIG_HMP_VARIABLE_SCALE=y
 CONFIG_HMP_FREQUENCY_INVARIANT_SCALE=y
