From d5f0ca9988c79c995d429e77897a9f6b1e72c1bb Mon Sep 17 00:00:00 2001
From: Xu Xuehui <xxh@rock-chips.com>
Date: Sat, 28 Apr 2018 10:52:28 +0800
Subject: [PATCH] ARM64: dts: rk3328-evb-android: decrease frequency sdio clk
 to 125M

Change-Id: I3cbbdbdcd97b18ec099a766ed3e08e8514f4932f
Signed-off-by: Xu Xuehui <xxh@rock-chips.com>
---
 arch/arm64/boot/dts/rockchip/rk3328-evb-android.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-evb-android.dts b/arch/arm64/boot/dts/rockchip/rk3328-evb-android.dts
index 7058b770701b..2ea2d5fdbd07 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-evb-android.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3328-evb-android.dts
@@ -563,7 +563,7 @@
 	cap-sdio-irq;
 	disable-wp;
 	keep-power-in-suspend;
-	max-frequency = <150000000>;
+	max-frequency = <125000000>;
 	mmc-pwrseq = <&sdio_pwrseq>;
 	non-removable;
 	num-slots = <1>;
-- 
