From c56df50fad8644c42d8dc11596ed9df7668beabb Mon Sep 17 00:00:00 2001
From: Lin Jianhua <linjh@rock-chips.com>
Date: Fri, 27 Jul 2018 17:50:35 +0800
Subject: [PATCH] ARM: dts: rockchip: rk3308-dot-v10-aarch32: wifi ext_clk_freq
 change to ref-clock-frequency

Change-Id: I2e796ba38736cc7da40436fcd8635f2ab4e0dbd5
Signed-off-by: Lin Jianhua <linjh@rock-chips.com>
---
 arch/arm/boot/dts/rk3308-dot-v10-aarch32.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/rk3308-dot-v10-aarch32.dts b/arch/arm/boot/dts/rk3308-dot-v10-aarch32.dts
index 57124b97caee..813a2d0fded4 100644
--- a/arch/arm/boot/dts/rk3308-dot-v10-aarch32.dts
+++ b/arch/arm/boot/dts/rk3308-dot-v10-aarch32.dts
@@ -164,7 +164,7 @@
 		rockchip,grf = <&grf>;
 		clocks = <&cru SCLK_WIFI>;
 		clock-names = "clk_wifi";
-		ext_clk_freq = <24000000>;
+		ref-clock-frequency = <24000000>;
 		pinctrl-names = "default";
 		pinctrl-0 = <&wifi_wake_host>;
 		wifi_chip_type = "rtl8723ds";
-- 
