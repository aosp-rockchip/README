 CONFIG_BACKLIGHT_PWM=y
 CONFIG_FB_ROCKCHIP=y
 CONFIG_LCDC_RK3368=y
-CONFIG_ROCKCHIP_RGA=y
-CONFIG_ROCKCHIP_RGA2=y
 CONFIG_RK_TRSM=y
 CONFIG_RK31XX_LVDS=y
 CONFIG_DP_ANX6345=y
 CONFIG_RK32_DP=y
