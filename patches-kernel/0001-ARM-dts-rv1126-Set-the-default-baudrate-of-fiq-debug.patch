From 666ef4643bcb18644e16a7e48082356205f82c31 Mon Sep 17 00:00:00 2001
From: Sugar Zhang <sugar.zhang@rock-chips.com>
Date: Wed, 4 Mar 2020 16:26:15 +0800
Subject: [PATCH] ARM: dts: rv1126: Set the default baudrate of fiq debugger to
 1.5MHz

Change-Id: Ide64e641088ed60149fef9bcfcb5cc1f760f8af8
Signed-off-by: Sugar Zhang <sugar.zhang@rock-chips.com>
---
 arch/arm/boot/dts/rv1126.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/rv1126.dtsi b/arch/arm/boot/dts/rv1126.dtsi
index 185ed210ce2c..f3b063e848e1 100644
--- a/arch/arm/boot/dts/rv1126.dtsi
+++ b/arch/arm/boot/dts/rv1126.dtsi
@@ -123,7 +123,7 @@
 		rockchip,serial-id = <2>;
 		rockchip,wake-irq = <0>;
 		rockchip,irq-mode-enable = <0>;
-		rockchip,baudrate = <115200>;  /* Only 115200 and 1500000 */
+		rockchip,baudrate = <1500000>;  /* Only 115200 and 1500000 */
 		interrupts = <GIC_SPI 93 IRQ_TYPE_LEVEL_HIGH>;
 		status = "disabled";
 	};
