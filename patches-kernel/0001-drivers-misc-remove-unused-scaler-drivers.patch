-static int parse_cmd(unsigned int cmd, unsigned long arg)
-{
-	printk("tv5735: parse scaler cmd %u\n",cmd);
-
-	switch (cmd) {
-		case SCALER_IOCTL_SET_CUR_INPUT:
-			set_cur_inport();
-			break;
-		default:
-			break;
-	}
-
-	return 0;
-}
