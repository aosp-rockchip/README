 arch/arm/boot/dts/rk628.dtsi | 1 +
 1 file changed, 1 insertion(+)

diff --git a/arch/arm/boot/dts/rk628.dtsi b/arch/arm/boot/dts/rk628.dtsi
index b25ee7770b4a..8c724f9c5d57 100644
--- a/arch/arm/boot/dts/rk628.dtsi
+++ b/arch/arm/boot/dts/rk628.dtsi
@@ -172,6 +172,7 @@
 			       "gpio3a3", /* vop_vsync */
 			       "gpio3b0"; /* vop_dclk */
 			function = "vop";
+			drive-strength = <1>;
 		};
 
 		hpdm1_out: hpdm1-out {
