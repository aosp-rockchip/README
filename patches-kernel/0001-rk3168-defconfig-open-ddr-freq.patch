From a5fcf3ae0377f6719ddaa9e260ecc36d17c547dc Mon Sep 17 00:00:00 2001
From: hjc <hjc@rock-chips.com>
Date: Wed, 27 Mar 2013 21:42:20 +0800
Subject: [PATCH] rk3168: defconfig open ddr freq

---
 arch/arm/configs/rk3168_86v_defconfig    | 2 --
 arch/arm/configs/rk3168_ds803f_defconfig | 2 --
 2 files changed, 4 deletions(-)

diff --git a/arch/arm/configs/rk3168_86v_defconfig b/arch/arm/configs/rk3168_86v_defconfig
index 5225f9fe2313..24be9e926567 100644
--- a/arch/arm/configs/rk3168_86v_defconfig
+++ b/arch/arm/configs/rk3168_86v_defconfig
@@ -23,7 +23,6 @@ CONFIG_MODULE_FORCE_UNLOAD=y
 CONFIG_ARCH_RK30=y
 CONFIG_DDR_INIT_CHANGE_FREQ=y
 CONFIG_DDR_SDRAM_FREQ=300
-# CONFIG_DDR_FREQ is not set
 # CONFIG_DDR_TEST is not set
 CONFIG_RK_CLOCK_PROC=y
 CONFIG_CLK_SWITCH_TO_32K=y
@@ -311,7 +310,6 @@ CONFIG_SND=y
 # CONFIG_SND_ARM is not set
 CONFIG_SND_SOC=y
 CONFIG_SND_RK29_SOC=y
-CONFIG_SND_RK29_SOC_I2S_2CH=y
 CONFIG_SND_I2S_DMA_EVENT_STATIC=y
 CONFIG_SND_RK29_SOC_RK610=y
 CONFIG_SND_RK29_CODEC_SOC_SLAVE=y
diff --git a/arch/arm/configs/rk3168_ds803f_defconfig b/arch/arm/configs/rk3168_ds803f_defconfig
index 314ed280d07a..082257431b88 100644
--- a/arch/arm/configs/rk3168_ds803f_defconfig
+++ b/arch/arm/configs/rk3168_ds803f_defconfig
@@ -23,7 +23,6 @@ CONFIG_MODULE_FORCE_UNLOAD=y
 CONFIG_ARCH_RK30=y
 CONFIG_DDR_INIT_CHANGE_FREQ=y
 CONFIG_DDR_SDRAM_FREQ=300
-# CONFIG_DDR_FREQ is not set
 # CONFIG_DDR_TEST is not set
 CONFIG_RK_CLOCK_PROC=y
 CONFIG_CLK_SWITCH_TO_32K=y
@@ -322,7 +321,6 @@ CONFIG_SND=y
 # CONFIG_SND_ARM is not set
 CONFIG_SND_SOC=y
 CONFIG_SND_RK29_SOC=y
-CONFIG_SND_RK29_SOC_I2S_2CH=y
 CONFIG_SND_I2S_DMA_EVENT_STATIC=y
 CONFIG_SND_RK29_SOC_ES8323=y
 CONFIG_SND_RK29_CODEC_SOC_SLAVE=y
-- 
