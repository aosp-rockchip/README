Date: Fri, 20 Apr 2012 14:31:28 +0800
Subject: [PATCH] rk30: defconfig: disable slub debug

---
 arch/arm/configs/rk30_phone_defconfig | 1 +
 arch/arm/configs/rk30_sdk_defconfig   | 1 +
 2 files changed, 2 insertions(+)

diff --git a/arch/arm/configs/rk30_phone_defconfig b/arch/arm/configs/rk30_phone_defconfig
index 559cdca7a0b8..9d05dcb348ab 100755
--- a/arch/arm/configs/rk30_phone_defconfig
+++ b/arch/arm/configs/rk30_phone_defconfig
@@ -17,6 +17,7 @@ CONFIG_PANIC_TIMEOUT=1
 CONFIG_ASHMEM=y
 # CONFIG_AIO is not set
 CONFIG_EMBEDDED=y
+# CONFIG_SLUB_DEBUG is not set
 CONFIG_MODULES=y
 CONFIG_MODULE_UNLOAD=y
 CONFIG_MODULE_FORCE_UNLOAD=y
diff --git a/arch/arm/configs/rk30_sdk_defconfig b/arch/arm/configs/rk30_sdk_defconfig
index 4ea3597c40b0..8b3d1a43fa0a 100644
--- a/arch/arm/configs/rk30_sdk_defconfig
+++ b/arch/arm/configs/rk30_sdk_defconfig
@@ -17,6 +17,7 @@ CONFIG_PANIC_TIMEOUT=1
 CONFIG_ASHMEM=y
 # CONFIG_AIO is not set
 CONFIG_EMBEDDED=y
+# CONFIG_SLUB_DEBUG is not set
 CONFIG_MODULES=y
 CONFIG_MODULE_UNLOAD=y
 CONFIG_MODULE_FORCE_UNLOAD=y
