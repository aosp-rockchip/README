From 7abe2657b73a2470efcae384c402e8b06594c9bd Mon Sep 17 00:00:00 2001
From: Grey Li <grey.li@rock-chips.com>
Date: Tue, 26 Nov 2019 19:48:27 +0800
Subject: [PATCH] ARM: dts: rockchip: change rkvdec version for rk322x

Fixes: 41ee9439d015 ("ARM: dts: rockchip: rk322x: dtsi for video codec")
Change-Id: I2ebb36b3f990a9420c418cc9ed9861be7bfd740f
Signed-off-by: Grey Li <grey.li@rock-chips.com>
---
 arch/arm/boot/dts/rk322x.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/rk322x.dtsi b/arch/arm/boot/dts/rk322x.dtsi
index b0292330e099..463017ce1bc3 100644
--- a/arch/arm/boot/dts/rk322x.dtsi
+++ b/arch/arm/boot/dts/rk322x.dtsi
@@ -864,7 +864,7 @@
 	};
 
 	rkvdec: rkvdec@20030000 {
-		compatible = "rockchip,rkv-decoder-v2";
+		compatible = "rockchip,rkv-decoder-v1";
 		reg = <0x20030000 0x400>;
 		interrupts = <GIC_SPI 7 IRQ_TYPE_LEVEL_HIGH>;
 		interrupt-names = "irq_dec";
