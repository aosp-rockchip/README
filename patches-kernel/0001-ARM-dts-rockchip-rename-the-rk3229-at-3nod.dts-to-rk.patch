From 8d714a30010dda361fc7f7c193beac74ac388f6f Mon Sep 17 00:00:00 2001
From: Yankun Zheng <zyk@rock-chips.com>
Date: Mon, 13 Nov 2017 11:41:50 +0800
Subject: [PATCH] ARM: dts: rockchip: rename the rk3229-at-3nod.dts to
 rk3229-at-gva.dts

Change-Id: I45358c6e2b707bd782b96d9fc4ca5c1ec3c7e1e3
Signed-off-by: Yankun Zheng <zyk@rock-chips.com>
---
 arch/arm/boot/dts/Makefile                                  | 2 +-
 arch/arm/boot/dts/{rk3229-at-3nod.dts => rk3229-at-gva.dts} | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)
 rename arch/arm/boot/dts/{rk3229-at-3nod.dts => rk3229-at-gva.dts} (99%)

diff --git a/arch/arm/boot/dts/Makefile b/arch/arm/boot/dts/Makefile
index a03d309eb5d3..5491c08f43c8 100644
--- a/arch/arm/boot/dts/Makefile
+++ b/arch/arm/boot/dts/Makefile
@@ -522,7 +522,7 @@ dtb-$(CONFIG_ARCH_ROCKCHIP) += \
 	rk3128-fireprime.dtb \
 	rk3188-radxarock.dtb \
 	rk3228-evb.dtb \
-	rk3229-at-3nod.dtb \
+	rk3229-at-gva.dtb \
 	rk3229-echo-v10.dtb \
 	rk3229-evb.dtb \
 	rk3229-gva-sdk.dtb \
diff --git a/arch/arm/boot/dts/rk3229-at-3nod.dts b/arch/arm/boot/dts/rk3229-at-gva.dts
similarity index 99%
rename from arch/arm/boot/dts/rk3229-at-3nod.dts
rename to arch/arm/boot/dts/rk3229-at-gva.dts
index 34dd20caf14b..3c4acf994b4d 100644
--- a/arch/arm/boot/dts/rk3229-at-3nod.dts
+++ b/arch/arm/boot/dts/rk3229-at-gva.dts
@@ -46,7 +46,7 @@
 
 / {
 	model = "RK3229 ANDROID THINGS 3NOD Board";
-	compatible = "rockchip,rk3229-at-3nod", "rockchip,rk3229";
+	compatible = "rockchip,rk3229-at-gva", "rockchip,rk3229";
 
 	sdio_pwrseq: sdio-pwrseq {
 		compatible = "mmc-pwrseq-simple";
-- 
2.35.3

