+void rk29_setgpio_suspend_board(void)
+{
+	gpio_set_request();
+	rk29_keygpio_suspend();
+	spi_gpio_suspend();
+	gpio_set_free();
+}
+
+void rk29_setgpio_resume_board(void)
+{
+	gpio_set_request();
+	rk29_keygpio_resume();
+	spi_gpio_resume();
+	gpio_set_free();
+}
