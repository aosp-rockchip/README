From 2c4256a61e10f0fad761b415b6b3faf77de3df9e Mon Sep 17 00:00:00 2001
From: Zorro Liu <lyx@rock-chips.com>
Date: Wed, 19 Dec 2018 17:17:37 +0800
Subject: [PATCH] arm64: dts: rockchip: modify audio card name for rk3368-p9
 and rk3368-xikp boards

Change-Id: I1e4354e7b3fca568fb14a7806b2f6f3734b9333b
Signed-off-by: Zorro Liu <lyx@rock-chips.com>
---
 arch/arm64/boot/dts/rockchip/rk3368-p9.dtsi   | 2 +-
 arch/arm64/boot/dts/rockchip/rk3368-xikp.dtsi | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3368-p9.dtsi b/arch/arm64/boot/dts/rockchip/rk3368-p9.dtsi
index 5bcdcedc4c95..33cbaa5bf894 100644
--- a/arch/arm64/boot/dts/rockchip/rk3368-p9.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3368-p9.dtsi
@@ -50,7 +50,7 @@
 	es8316-sound {
 		compatible = "simple-audio-card";
 		simple-audio-card,format = "i2s";
-		simple-audio-card,name = "rockchip,es8316-codec";
+		simple-audio-card,name = "rockchip,rk-es8316-codec";
 		simple-audio-card,mclk-fs = <256>;
 		simple-audio-card,widgets =
 			"Microphone", "Mic Jack",
diff --git a/arch/arm64/boot/dts/rockchip/rk3368-xikp.dtsi b/arch/arm64/boot/dts/rockchip/rk3368-xikp.dtsi
index 325b3ddac457..57c5276264f4 100644
--- a/arch/arm64/boot/dts/rockchip/rk3368-xikp.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3368-xikp.dtsi
@@ -50,7 +50,7 @@
 	es8316-sound {
 		compatible = "simple-audio-card";
 		simple-audio-card,format = "i2s";
-		simple-audio-card,name = "rockchip,es8316-codec";
+		simple-audio-card,name = "rockchip,rk-es8316-codec";
 		simple-audio-card,mclk-fs = <256>;
 		simple-audio-card,widgets =
 			"Microphone", "Mic Jack",
-- 
