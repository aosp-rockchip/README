+#define SPI_CHIPSELECT_NUM 3
+struct spi_cs_gpio rk2818_spi_cs_gpios[SPI_CHIPSELECT_NUM] = {
+	{
+		.name = "spi cs0",
+		.cs_gpio = RK2818_PIN_PB4,
+		.cs_iomux_name = GPIOB4_SPI0CS0_MMC0D4_NAME,//if no iomux,set it NULL
+		.cs_iomux_mode = IOMUXA_GPIO0_B4,
