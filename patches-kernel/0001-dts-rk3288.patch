From: lintao <lintao@rock-chips.com>
Date: Wed, 11 Jun 2014 12:58:18 +0800
Subject: [PATCH] dts: rk3288:  	sd/mmc: fix spell error

---
 arch/arm/boot/dts/rk3288-p977.dts | 2 +-
 arch/arm/boot/dts/rk3288-tb.dts   | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/rk3288-p977.dts b/arch/arm/boot/dts/rk3288-p977.dts
index 7408e27d5f1f..c52be8ca1704 100755
--- a/arch/arm/boot/dts/rk3288-p977.dts
+++ b/arch/arm/boot/dts/rk3288-p977.dts
@@ -193,7 +193,7 @@
     
 &sdmmc {
 	clock-frequency = <50000000>;
-	lock-freq-min-max = <400000 50000000>;	            
+	clock-freq-min-max = <400000 50000000>;	            
 	    
 	supports-highspeed;
 	supports-sd;
diff --git a/arch/arm/boot/dts/rk3288-tb.dts b/arch/arm/boot/dts/rk3288-tb.dts
index 1e1053787710..e6b8c61341d7 100755
--- a/arch/arm/boot/dts/rk3288-tb.dts
+++ b/arch/arm/boot/dts/rk3288-tb.dts
@@ -228,7 +228,7 @@
     
 &sdmmc {
 		clock-frequency = <50000000>;
-		lock-freq-min-max = <400000 50000000>;
+		clock-freq-min-max = <400000 50000000>;
 		supports-highspeed;
 		supports-sd;
 		broken-cd;
